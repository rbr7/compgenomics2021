/home/aaron/geneprediction/Code/genemarks2/gmhmmp2 -M /home/aaron/geneprediction/Code/genemarks2/mgm_11.mod -s /home/aaron/geneprediction/Data2/CGT1009_careful_Scaffolds.fasta -o tmp_mgm_11.gff -f gff
echo '>anydef' > tmpseq.fna
grep -v '>' /home/aaron/geneprediction/Data2/CGT1009_careful_Scaffolds.fasta | tr '[:lower:]' '[:upper:]' >> tmpseq.fna
/home/aaron/geneprediction/Code/genemarks2/gmhmmp2 -M /home/aaron/geneprediction/Code/genemarks2/mgm_11.mod -s tmpseq.fna -o itr_0.lst 
/home/aaron/geneprediction/Code/genemarks2/biogem gms2-training -s tmpseq.fna -l itr_0.lst -m itr_1.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 0 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --run-motif-search false --genome-group D
/home/aaron/geneprediction/Code/genemarks2/gmhmmp2 -m itr_1.mod -M /home/aaron/geneprediction/Code/genemarks2/mgm_11.mod -s tmpseq.fna -o itr_1.lst --format train
/home/aaron/geneprediction/Code/genemarks2/compp -n -a itr_0.lst -b itr_1.lst -G
/home/aaron/geneprediction/Code/genemarks2/biogem gms2-training -s tmpseq.fna -l itr_1.lst -m itr_2.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group C --gc-upstr-len-rbs 20 --align right --gc-width-rbs 6 --gc-upstr-len-prom 20 --gc-width-prom 6 --gc-extended-sd TAAGGAGGTGA
/home/aaron/geneprediction/Code/genemarks2/gmhmmp2 -m itr_2.mod -M /home/aaron/geneprediction/Code/genemarks2/mgm_11.mod -s tmpseq.fna -o itr_2.lst --format train
/home/aaron/geneprediction/Code/genemarks2/compp -n -a itr_1.lst -b itr_2.lst -G
/home/aaron/geneprediction/Code/genemarks2/biogem experiment promoter-is-valid-for-bacteria --fnmod itr_2.mod --dist-thresh 14 --score-thresh 0.25 --window-size 1 --min-leaderless-percent 11 --min-leaderless-count 100 --fnlabels itr_2.lst --fnseq tmpseq.fna
/home/aaron/geneprediction/Code/genemarks2/biogem experiment promoter-and-rbs-match --fnmod itr_2.mod --match-thresh 3
/home/aaron/geneprediction/Code/genemarks2/biogem gms2-training -s tmpseq.fna -l itr_1.lst -m itr_2.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group B --gb-upstr-len-rbs 20 --align right --gb-width-rbs 6
/home/aaron/geneprediction/Code/genemarks2/gmhmmp2 -m itr_2.mod -M /home/aaron/geneprediction/Code/genemarks2/mgm_11.mod -s tmpseq.fna -o itr_2.lst --format train
/home/aaron/geneprediction/Code/genemarks2/compp -n -a itr_1.lst -b itr_2.lst -G
/home/aaron/geneprediction/Code/genemarks2/biogem experiment rbs-consensus-and-16s-match --fnmod itr_2.mod --allow-ag-sub
/home/aaron/geneprediction/Code/genemarks2/biogem gms2-training -s tmpseq.fna -l itr_1.lst -m itr_2.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group A --ga-upstr-len-rbs 20 --align right --ga-width-rbs 6
/home/aaron/geneprediction/Code/genemarks2/gmhmmp2 -m itr_2.mod -M /home/aaron/geneprediction/Code/genemarks2/mgm_11.mod -s tmpseq.fna -o itr_2.lst --format train
/home/aaron/geneprediction/Code/genemarks2/compp -n -a itr_1.lst -b itr_2.lst -G
/home/aaron/geneprediction/Code/genemarks2/biogem experiment match-rbs-to-16s --fnlabels itr_2.lst --match-to TAAGGAGGTGA --min-match 4 --allow-ag-sub
/home/aaron/geneprediction/Code/genemarks2/biogem gms2-training -s tmpseq.fna -l itr_2.lst -m itr_3.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group A --ga-upstr-len-rbs 20 --align right --ga-width-rbs 6
